		System.out.println("Implementacja");
		KlientDAOImp klientIm = new KlientDAOImp();
		
		System.out.println("Dodaj klient");
		klientIm.add(klient);
		
		List<Klient> dane = klientIm.getKlient();
		
		
		
	} 
}